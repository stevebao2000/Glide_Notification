<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners
        android:radius="5dp"/>

    <gradient
        android:angle="-90"
        android:startColor="#e7fffbed"
        android:centerColor="#d2fff1b8"
        android:endColor="#ccffea99"
        android:type="linear" />

    <stroke
        android:dashGap="0dp"
        android:dashWidth="0dp"
        android:width="1dp"
        android:color="#eccb70" />

</shape>

